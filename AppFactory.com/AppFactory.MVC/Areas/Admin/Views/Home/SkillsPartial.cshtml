@model IEnumerable<AppFactory.DAL.Applicant_Skill>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Skill.SkillName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SkillLevel)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Skill.SkillName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SkillLevel)
        </td>
    </tr>
}
</table>
