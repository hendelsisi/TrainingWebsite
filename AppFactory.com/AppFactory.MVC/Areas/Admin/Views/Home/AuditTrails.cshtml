@model IEnumerable<AppFactory.DAL.AuditTrail>
@{
    ViewBag.Title = "AuditTrails";
}

<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Audit Trails</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                    <table id="example2" class="table table-bordered table-hover">
                        <thead>
                        <tr>
                            <th>
                                @Html.DisplayName("User Name")
                            </th>
                            <th>
                                @Html.DisplayName("Area Accessed")
                            </th>
                            <th>
                                @Html.DisplayName("IP Address")
                            </th>
                            <th>
                                @Html.DisplayName("Time")
                            </th>
                        </tr>
                            </thead>
                        <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.UserName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AreaAccessed)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.IPAddress)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Time)
                                </td>
                            </tr>
                        }
                        </tbody>
                        @*<tfoot>
                                <tr>
                                    <th>Rendering engine</th>
                                    <th>Browser</th>
                                </tr>
                            </tfoot>*@
                    </table>
                </div><!-- /.box-body -->
            </div><!-- /.box -->
        </div>
    </div>

</section>
